{
  "name": "qoder-wiki-export",
  "displayName": "Qoder Wiki Export",
  "description": "Export Qoder-generated repository documentation to various formats",
  "version": "0.1.0",
  "publisher": "qoder-wiki-export",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "qoder",
    "wiki",
    "export",
    "documentation",
    "markdown"
  ],
  "activationEvents": [
    "onCommand:qoderWikiExport.exportWiki",
    "onCommand:qoderWikiExport.exportSelected",
    "onCommand:qoderWikiExport.checkQoderStatus",
    "onCommand:qoderWikiExport.showInfo"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qoderWikiExport.exportWiki",
        "title": "Export Qoder Wiki Documentation",
        "category": "Qoder Wiki Export",
        "icon": "$(export)"
      },
      {
        "command": "qoderWikiExport.exportSelected",
        "title": "Export Selected Wiki Documents",
        "category": "Qoder Wiki Export",
        "icon": "$(export)"
      },
      {
        "command": "qoderWikiExport.checkQoderStatus",
        "title": "Check Qoder Status",
        "category": "Qoder Wiki Export",
        "icon": "$(pulse)"
      },
      {
        "command": "qoderWikiExport.showInfo",
        "title": "Show Extension Info",
        "category": "Qoder Wiki Export",
        "icon": "$(info)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {},
  "extensionDependencies": [
    "aicoding.aicoding-agent"
  ]
}